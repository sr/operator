# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Local secrets
/.envvars_*.rb
!/.envvars_sample.rb

# Compiled assets
/public/assets

# Failed tests
/spec/examples.txt

# rubymine
.idea
.idea/*

# Docker
/.devenv.sync.tmp
/vendor/gems
